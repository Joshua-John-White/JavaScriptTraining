<p id="p2"></p>

<button type="button" onclick="u()">Upper Case</button>


<script>

document.getElementById("p1").innerHTML =  "He said 'My name is Elliott'. ";

function u() {

var lower = "He said 'My name is Elliott'. ";
var upper= lower.toUpperCase();

document.getElementById("p2").innerHTML = upper;
return;
}

</script>

