<!DOCTYPE html>
<html>
<body>

<h1>Level 1 - Basic</h1>

<p><strong>Variables 2</strong></p>



Name:		 <input type="string" id="name">
<p>
Age: 		 <input type="number" id="age">
<p>
Occupation 	 <input type="string" id="occ">
<p>

<button onclick="submit()">Submit</button>

<p id="printName"></p>
<p id="printAge"></p>
<p id="printOcc"></p>

<script> 

function submit() {
var name = document.getElementById("name").value;
var age = document.getElementById("age").value;
var occ = document.getElementById("occ").value;

 document.getElementById("printName").innerHTML = name;
  document.getElementById("printAge").innerHTML = age;
   document.getElementById("printOcc").innerHTML = occ;
 } 
 
</script>

